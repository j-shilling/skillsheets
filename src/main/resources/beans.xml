<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config/>
	
	<bean id="GoogleIdTokenVerifier" class="com.shilling.skillsheets.api.services.GoogleIdTokenVerifierFactory"/>
	<bean id="Userservice" class="com.shilling.skillsheets.api.services.GoogleUserService"/>
	<bean id="SkillSheetService" class="com.shilling.skillsheets.api.services.SkillSheetServiceImpl"/>
	<bean id="UserDao" class="com.shilling.skillsheets.dao.local.LocalUserDaoFactory"/>
	<bean id="SkillSheetDao" class="com.shilling.skillsheets.dao.local.LocalSkillSheetDao"/>
   
</beans>